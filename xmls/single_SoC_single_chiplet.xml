<AR-PACKAGE>
	<ELEMENTS>
		<Simulation-Time simtime="1000" />
		<OS>
			<Nucleus_RTOS />
		</OS>
		<SENDER-RECEIVER-INTERFACE>
			<SHORT-NAME name="Interface1" ID="Interface1" Buffer-Size="5"/>
			<DATA-ELEMENTS>
				<VARIABLE-DATA-PROTOTYPE>
					<SHORT-NAME name="Data1" ID="Data1"/>
					<TYPE-TREF>
						<Array-of-uint8 No_Of_Bytes="100">
							<Random-Values-generated BoundaryRandomValue_Enable="true">
								<Data-Range min="1" max="100"/>
							</Random-Values-generated>
						</Array-of-uint8>
					</TYPE-TREF>
				</VARIABLE-DATA-PROTOTYPE>
			</DATA-ELEMENTS>
		</SENDER-RECEIVER-INTERFACE>

		<APPLICATION-SW-COMPONENT-TYPE>
			<SHORT-NAME name="SWC1" ID="SWC1"/>
			<PORTS>
				<P-PORT-PROTOTYPE>
					<SHORT-NAME name="PP1" ID="PP1"/>
					<PROVIDED-INTERFACE-TREF DEST="Interface1"/>
				</P-PORT-PROTOTYPE>
			</PORTS>
			<INTERNAL-BEHAVIORS>
				<SWC-INTERNAL-BEHAVIOR>
					<SHORT-NAME name="INTERNAL_BEH1" ID="INTERNAL_BEH1"/>
					<RUNNABLES>
						<RUNNABLE-ENTITY>
							<SHORT-NAME name="Runnable1" ID="Runnable1"/>
							<DATA-WRITE-ACCESSS>
								<VARIABLE-ACCESS>
									<SHORT-NAME name="WData1" ID="WData1"/>
									<ACCESSED-VARIABLE>
										<AUTOSAR-VARIABLE-IREF>
											<PORT-PROTOTYPE-REF DEST="/SWC1/PP1"/>
											<TARGET-DATA-PROTOTYPE-REF DEST="/Interface1/Data1"/>
										</AUTOSAR-VARIABLE-IREF>
									</ACCESSED-VARIABLE>
								</VARIABLE-ACCESS>
							</DATA-WRITE-ACCESSS>
						</RUNNABLE-ENTITY>
					</RUNNABLES>
					<EVENTS>
						<TIMING-EVENT>
							<SHORT-NAME name="TimedEvent" ID="TimedEvent"/>
							<START-ON-EVENT-REF DEST="/SWC1/INTERNAL_BEH1/Runnable1"/>
							<PERIOD value="0.16"/>
						</TIMING-EVENT>
					</EVENTS>
				</SWC-INTERNAL-BEHAVIOR>
			</INTERNAL-BEHAVIORS>
		</APPLICATION-SW-COMPONENT-TYPE>
		<APPLICATION-SW-COMPONENT-TYPE>
			<SHORT-NAME name="SWC2" ID="SWC2"/>
			<PORTS>
				<R-PORT-PROTOTYPE>
					<SHORT-NAME name="RP1" ID="RP1"/>
					<REQUIRED-INTERFACE-TREF DEST="Interface1"/>
				</R-PORT-PROTOTYPE>
			</PORTS>
			<INTERNAL-BEHAVIORS>
				<SWC-INTERNAL-BEHAVIOR>
					<SHORT-NAME name="INTERNAL_BEH2" ID="INTERNAL_BEH2"/>
					<RUNNABLES>
						<RUNNABLE-ENTITY>
							<SHORT-NAME name="Runnable2" ID="Runnable2"/>
							<DATA-READ-ACCESSS>
								<VARIABLE-ACCESS>
									<SHORT-NAME name="RData2" ID="RData2"/>
									<ACCESSED-VARIABLE>
										<AUTOSAR-VARIABLE-IREF>
											<PORT-PROTOTYPE-REF DEST="/SWC2/RP1"/>
											<TARGET-DATA-PROTOTYPE-REF DEST="/Interface1/Data1"/>
										</AUTOSAR-VARIABLE-IREF>
									</ACCESSED-VARIABLE>
								</VARIABLE-ACCESS>
							</DATA-READ-ACCESSS>
						</RUNNABLE-ENTITY>
					</RUNNABLES>
					<EVENTS>
						<DATA-RECEIVED-EVENT>
							<SHORT-NAME name="ReceivedEvent1" ID="ReceivedEvent1"/>
							<START-ON-EVENT-REF DEST="/SWC2/INTERNAL_BEH2/Runnable2"/>
							<DATA-IREF>
								<TARGET-DATA-ELEMENT-REF DEST="/SWC2/INTERNAL_BEH2/Runnable2/RData2"/>
							</DATA-IREF>
						</DATA-RECEIVED-EVENT>
					</EVENTS>
				</SWC-INTERNAL-BEHAVIOR>
			</INTERNAL-BEHAVIORS>
		</APPLICATION-SW-COMPONENT-TYPE>

		<ECUs>
			<SHORT-NAME name="ECU1" ID="ECU1"/>
			<SoCs>
				<SHORT-NAME name="SoC1" ID="SoC1"/>
				<Chiplet>
					<SHORT-NAME name="Chiplet1" ID="Chiplet1"/>				
					<CPU_Cluster>
						<ARMV8-Family>
							<CortexA72>
								<SHORT-NAME name="CortexA72_1" ID="CortexA72_1"/>
								<Frequency name="1000000000"/>
								<Core>
									<SHORT-NAME name="Core1" ID="Core1"/>
									<Core-runnable-Mapping DEST="/SWC1/INTERNAL_BEH1/Runnable1" Load="100" priority="8" />
								</Core>
								<Core>
									<SHORT-NAME name="Core2" ID="Core2"/>
									<Core-runnable-Mapping DEST="/SWC2/INTERNAL_BEH2/Runnable2" Load="100" priority="8" />
								</Core>
							</CortexA72>
						</ARMV8-Family>
					</CPU_Cluster>
				</Chiplet>
			</SoCs>
		</ECUs>
		<Analysis>
			<Analysis-Enable>
				<ALL-ECUs Enable="true">
					<Analysis-output-directory Dir-Location=" "/>
					<Analyze_all_Sim_Time></Analyze_all_Sim_Time>
				</ALL-ECUs>
			</Analysis-Enable>
		</Analysis>
	</ELEMENTS>
</AR-PACKAGE>
