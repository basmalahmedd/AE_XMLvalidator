                                    <AUTOSAR-VARIABLE-IREF>
                                        <PORT-PROTOTYPE-REF DEST="/CPUComponent1/HWIP_to_Cluster1_3_Port"/>
                                        <TARGET-DATA-PROTOTYPE-REF DEST="/HWIP_to_Cluster1_3_Interface/HWIP_to_Cluster1_3_Variable"/>
                                    </AUTOSAR-VARIABLE-IREF>
                                </ACCESSED-VARIABLE>
                            </VARIABLE-ACCESS>
                        </DATA-READ-ACCESS>

                        <DATA-WRITE-ACCESS>
                            <VARIABLE-ACCESS>
                                <SHORT-NAME name="WritePortVariable16"/>
                                <ACCESSED-VARIABLE>
