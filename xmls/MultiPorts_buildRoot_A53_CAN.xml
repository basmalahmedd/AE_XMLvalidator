                        </DATA-READ-ACCESS>
                        <DATA-READ-ACCESS>
                            <VARIABLE-ACCESS>
                                <SHORT-NAME name="ReadPortVariable17"/>
                                <ACCESSED-VARIABLE>
                                    <AUTOSAR-VARIABLE-IREF>
                                        <PORT-PROTOTYPE-REF DEST="/CPUComponent2/HWIP_to_Cluster1_17_Port"/>
                                        <TARGET-DATA-PROTOTYPE-REF DEST="/HWIP_to_Cluster1_17_Interface/HWIP_to_Cluster1_17_Variable"/>
                                    </AUTOSAR-VARIABLE-IREF>
                                </ACCESSED-VARIABLE>
                            </VARIABLE-ACCESS>
                        </DATA-READ-ACCESS>        
