                <PROVIDED-INTERFACE-TREF DEST="/HWIP_to_Cluster1_16_Interface"/>
            </P-PORT-PROTOTYPE>
            <P-PORT-PROTOTYPE>
                <SHORT-NAME name="HWIP_to_Cluster1_17_Port"/>
                <PROVIDED-INTERFACE-TREF DEST="/HWIP_to_Cluster1_17_Interface"/>
            </P-PORT-PROTOTYPE>

        </PORTS>
        <INTERNAL-BEHAVIORS>
            <SWC-INTERNAL-BEHAVIOR>
                <SHORT-NAME name="CPUBehavior"/>
                <RUNNABLES>
                    <RUNNABLE-ENTITY>
                        <SHORT-NAME name="Runnable_HandlingHWIPsEvents"/>
                        <DATA-READ-ACCESS>
                            <VARIABLE-ACCESS>
                                <SHORT-NAME name="ReadPortVariable0"/>
                                <ACCESSED-VARIABLE>
                                    <AUTOSAR-VARIABLE-IREF>
                                        <PORT-PROTOTYPE-REF DEST="/CPUComponent1/HWIP_to_Cluster1_0_Port"/>
                                        <TARGET-DATA-PROTOTYPE-REF DEST="/HWIP_to_Cluster1_0_Interface/HWIP_to_Cluster1_0_Variable"/>
                                    </AUTOSAR-VARIABLE-IREF>
                                </ACCESSED-VARIABLE>
                            </VARIABLE-ACCESS>
                        </DATA-READ-ACCESS>
                        <DATA-READ-ACCESS>
                            <VARIABLE-ACCESS>
                                <SHORT-NAME name="ReadPortVariable1"/>
                                <ACCESSED-VARIABLE>
                                    <AUTOSAR-VARIABLE-IREF>
                                        <PORT-PROTOTYPE-REF DEST="/CPUComponent1/HWIP_to_Cluster1_1_Port"/>
                                        <TARGET-DATA-PROTOTYPE-REF DEST="/HWIP_to_Cluster1_1_Interface/HWIP_to_Cluster1_1_Variable"/>
                                    </AUTOSAR-VARIABLE-IREF>
                                </ACCESSED-VARIABLE>
                            </VARIABLE-ACCESS>
                        </DATA-READ-ACCESS>
                        <DATA-READ-ACCESS>
                            <VARIABLE-ACCESS>
                                <SHORT-NAME name="ReadPortVariable2"/>
                                <ACCESSED-VARIABLE>
