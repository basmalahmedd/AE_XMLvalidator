                                </ACCESSED-VARIABLE>
                            </VARIABLE-ACCESS>
                        </DATA-WRITE-ACCESS>
                        <DATA-WRITE-ACCESS>
                            <VARIABLE-ACCESS>
                                <SHORT-NAME name="WritePortVariable17"/>
                                <ACCESSED-VARIABLE>
                                    <AUTOSAR-VARIABLE-IREF>
                                        <PORT-PROTOTYPE-REF DEST="/CPUComponent1/HWIP_to_Cluster1_17_Port"/>
                                        <TARGET-DATA-PROTOTYPE-REF DEST="/HWIP_to_Cluster1_17_Interface/HWIP_to_Cluster1_17_Variable"/>
                                    </AUTOSAR-VARIABLE-IREF>
                                </ACCESSED-VARIABLE>
                            </VARIABLE-ACCESS>
                        </DATA-WRITE-ACCESS>
                    </RUNNABLE-ENTITY>
                </RUNNABLES>

                <EVENTS>
                    <DATA-RECEIVED-EVENT>
                        <SHORT-NAME name="DRE0" />
                        <START-ON-EVENT-REF DEST="/CPUComponent1/CPUBehavior/Runnable_HandlingHWIPsEvents" />
                        <CUSTOM-BEHAVIOR-REF DEST="CustomSWBeh0" />
                        <DATA-IREF>
                            <TARGET-DATA-ELEMENT-REF DEST="/CPUComponent1/CPUBehavior/Runnable_HandlingHWIPsEvents/ReadPortVariable0" />
                        </DATA-IREF>
                    </DATA-RECEIVED-EVENT>
                    <DATA-RECEIVED-EVENT>
                        <SHORT-NAME name="DRE1" />
                        <START-ON-EVENT-REF DEST="/CPUComponent1/CPUBehavior/Runnable_HandlingHWIPsEvents" />
                        <CUSTOM-BEHAVIOR-REF DEST="CustomSWBeh0" />
                        <DATA-IREF>
                            <TARGET-DATA-ELEMENT-REF DEST="/CPUComponent1/CPUBehavior/Runnable_HandlingHWIPsEvents/ReadPortVariable1" />
                        </DATA-IREF>
                    </DATA-RECEIVED-EVENT>
                    <DATA-RECEIVED-EVENT>
                        <SHORT-NAME name="DRE2" />
                        <START-ON-EVENT-REF DEST="/CPUComponent1/CPUBehavior/Runnable_HandlingHWIPsEvents" />
                        <CUSTOM-BEHAVIOR-REF DEST="CustomSWBeh0" />
                        <DATA-IREF>
                            <TARGET-DATA-ELEMENT-REF DEST="/CPUComponent1/CPUBehavior/Runnable_HandlingHWIPsEvents/ReadPortVariable2" />
                        </DATA-IREF>
                    </DATA-RECEIVED-EVENT>
                    <DATA-RECEIVED-EVENT>
                        <SHORT-NAME name="DRE3" />
                        <START-ON-EVENT-REF DEST="/CPUComponent1/CPUBehavior/Runnable_HandlingHWIPsEvents" />
                        <CUSTOM-BEHAVIOR-REF DEST="CustomSWBeh0" />
                        <DATA-IREF>
