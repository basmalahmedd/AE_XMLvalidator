                    <DATA-RECEIVED-EVENT>
                        <SHORT-NAME name="DRE17" />
                        <START-ON-EVENT-REF DEST="/CPUComponent2/CPUBehavior/Runnable_HandlingHWIPsEvents_2" />
                        <DATA-IREF>
                            <TARGET-DATA-ELEMENT-REF DEST="/CPUComponent2/CPUBehavior/Runnable_HandlingHWIPsEvents_2/ReadPortVariable17" />
                        </DATA-IREF>
                    </DATA-RECEIVED-EVENT>
                </EVENTS>
            </SWC-INTERNAL-BEHAVIOR>
        </INTERNAL-BEHAVIORS>
    </APPLICATION-SW-COMPONENT-TYPE>

    <SWC-CUSTOM-BEHAVIOR>
        <SHORT-NAME name="CustomSWBeh0" />
        <OPERATIONS-SEQUENCE>
            <OPERATION>
                <LOAD value="20000" />
            </OPERATION>
        </OPERATIONS-SEQUENCE>
    </SWC-CUSTOM-BEHAVIOR>

    <SWC-CUSTOM-BEHAVIOR>
      <SHORT-NAME name="CustomSWBeh1" />
      <OPERATIONS-SEQUENCE>
        <!-- <OPERATION>
          <CONVOLUTION />
        </OPERATION> -->

        <OPERATION>
          <WRITE>
            <IREF DEST="/CPUComponent1/CPUBehavior/Runnable_HandlingHWIPsEvents/WritePortVariable16" />
          </WRITE>
        </OPERATION>
        <OPERATION>
          <WRITE>
            <IREF DEST="/CPUComponent1/CPUBehavior/Runnable_HandlingHWIPsEvents/WritePortVariable17" />
          </WRITE>
        </OPERATION>

      </OPERATIONS-SEQUENCE>
    </SWC-CUSTOM-BEHAVIOR>


    <Network-Topology>
        <InterECU_communication>
            <CAN-BUS CAN-FD="false">
                <SHORT-NAME name="CAN_BUS1" />
                <BaudRate />
                <INTERFACE-TREF DEST="HWIP_to_Cluster1_16_Interface" />
            </CAN-BUS>
            <CAN-BUS CAN-FD="false">
                <SHORT-NAME name="CAN_BUS2" />
                <BaudRate />
                <INTERFACE-TREF DEST="HWIP_to_Cluster1_17_Interface" />
            </CAN-BUS>
        </InterECU_communication>
    </Network-Topology>


    <ECUs>
        <SHORT-NAME name="ECU0"/>
        <SoCs>
            <SHORT-NAME name="SoC0"/>
            <AXI-BUS width="4" frequency="100000000"/>
			<ETHERNET-INTERFACE />
			<UCIe-INTERFACE Mode="host" />
            <CPU_Cluster>
                <Operating-System>
                    <Linux>
                        <Buildroot_File_System/>
                    </Linux>
                </Operating-System>
                <ARMV8-Family>
                    <CortexA53 CoresPerCluster="1">
                        <SHORT-NAME name="CPUCluster1"/>
                        <Frequency value="1000000000"/>
                        <!-- <Core>
                            <SHORT-NAME name="CPUCore0"/>
                            <Core-runnable-Mapping DEST="/CPUComponent1/CPUBehavior/Runnable_HandlingHWIPsEvents"    Load="1000" priority="1"/>
                        </Core> -->
                    </CortexA53>
                </ARMV8-Family>
                <!-- <POWER-PARAMETERS /> -->
            </CPU_Cluster>
            <Generic_Hardware>
                <SHORT-NAME name="HWIP0" />
                <Frequency value="1000000000"/>
                <AXI-Master-Port priority="1" />
                <MEMORY-INTERFACE />
                <INTERNAL-BEHAVIOR>
                    <PORTS>
                        <P-PORT-PROTOTYPE>
                            <SHORT-NAME name="RPORT" />
                            <PROVIDED-INTERFACE-TREF DEST="HWIP_to_Cluster1_0_Interface" />
                        </P-PORT-PROTOTYPE>
                    </PORTS>
                    <OPERATIONS-SEQUENCE>
                        <OPERATION>
                            <WRITE>
                                <IREF DEST="/HWIP0/RPORT" />
                            </WRITE>
                        </OPERATION>
                        <OPERATION>
                            <LATENCY value="10" unit="us" />
                        </OPERATION>
                    </OPERATIONS-SEQUENCE>
                    <EVENT>
                        <TIMING-EVENT>
                            <PERIOD value="200" unit="ms" />
                        </TIMING-EVENT>
                    </EVENT>
                </INTERNAL-BEHAVIOR>
                <!-- <POWER-PARAMETERS /> -->
            </Generic_Hardware>                
            <Generic_Hardware>
                <SHORT-NAME name="HWIP1" />
                <Frequency value="1000000000"/>
                <AXI-Master-Port priority="1" />
                <MEMORY-INTERFACE />
                <INTERNAL-BEHAVIOR>
                    <PORTS>
                        <P-PORT-PROTOTYPE>
                            <SHORT-NAME name="RPORT" />
                            <PROVIDED-INTERFACE-TREF DEST="HWIP_to_Cluster1_1_Interface" />
                        </P-PORT-PROTOTYPE>
                    </PORTS>
                    <OPERATIONS-SEQUENCE>
                        <OPERATION>
                            <WRITE>
                                <IREF DEST="/HWIP1/RPORT" />
                            </WRITE>
                        </OPERATION>
                        <OPERATION>
                            <LATENCY value="10" unit="us" />
                        </OPERATION>
                    </OPERATIONS-SEQUENCE>
                    <EVENT>
                        <TIMING-EVENT>
                            <PERIOD value="250" unit="ms" />
                        </TIMING-EVENT>
                    </EVENT>
                </INTERNAL-BEHAVIOR>
                <!-- <POWER-PARAMETERS /> -->
            </Generic_Hardware>                
            <Generic_Hardware>
                <SHORT-NAME name="HWIP2" />
                <Frequency value="1000000000"/>
                <AXI-Master-Port priority="1" />
                <MEMORY-INTERFACE />
                <INTERNAL-BEHAVIOR>
                    <PORTS>
                        <P-PORT-PROTOTYPE>
                            <SHORT-NAME name="RPORT" />
                            <PROVIDED-INTERFACE-TREF DEST="HWIP_to_Cluster1_2_Interface" />
                        </P-PORT-PROTOTYPE>
                    </PORTS>
                    <OPERATIONS-SEQUENCE>
                        <OPERATION>
                            <WRITE>
                                <IREF DEST="/HWIP2/RPORT" />
                            </WRITE>
                        </OPERATION>
                        <OPERATION>
                            <LATENCY value="10" unit="us" />
                        </OPERATION>
                    </OPERATIONS-SEQUENCE>
                    <EVENT>
                        <TIMING-EVENT>
                            <PERIOD value="300" unit="ms" />
                        </TIMING-EVENT>
                    </EVENT>
                </INTERNAL-BEHAVIOR>
                <!-- <POWER-PARAMETERS /> -->
            </Generic_Hardware>                
            <Generic_Hardware>
                <SHORT-NAME name="HWIP3" />
                <Frequency value="1000000000"/>
                <AXI-Master-Port priority="1" />
                <MEMORY-INTERFACE />
                <INTERNAL-BEHAVIOR>
                    <PORTS>
                        <P-PORT-PROTOTYPE>
                            <SHORT-NAME name="RPORT" />
                            <PROVIDED-INTERFACE-TREF DEST="HWIP_to_Cluster1_3_Interface" />
                        </P-PORT-PROTOTYPE>
                    </PORTS>
                    <OPERATIONS-SEQUENCE>
                        <OPERATION>
                            <WRITE>
                                <IREF DEST="/HWIP3/RPORT" />
                            </WRITE>
                        </OPERATION>
                        <OPERATION>
                            <LATENCY value="10" unit="us" />
                        </OPERATION>
                    </OPERATIONS-SEQUENCE>
                    <EVENT>
                        <TIMING-EVENT>
                            <PERIOD value="350" unit="ms" />
                        </TIMING-EVENT>
                    </EVENT>
                </INTERNAL-BEHAVIOR>
                <!-- <POWER-PARAMETERS /> -->
            </Generic_Hardware>
        </SoCs>
    </ECUs>
    
    <ECUs>
        <SHORT-NAME name="ECU1"/>
        <SoCs>
            <SHORT-NAME name="SoC1"/>
            <AXI-BUS width="4" frequency="100000000"/>
			<ETHERNET-INTERFACE />
			<UCIe-INTERFACE Mode="host" />
            <CPU_Cluster>
                <Operating-System>
                    <Linux>
                        <Buildroot_File_System/>
                    </Linux>
                </Operating-System>
                <ARMV8-Family>
                    <CortexA53 CoresPerCluster="1">
                        <SHORT-NAME name="CPUCluster1"/>
                        <Frequency value="1000000000"/>
                        <!-- <Core>
                            <SHORT-NAME name="CPUCore0"/>
                            <Core-runnable-Mapping DEST="/CPUComponent2/CPUBehavior/Runnable_HandlingHWIPsEvents_2"    Load="1000" priority="1"/>
                        </Core> -->
                    </CortexA53>
                </ARMV8-Family>
                <!-- <POWER-PARAMETERS /> -->
            </CPU_Cluster>

        </SoCs>
    </ECUs>

    <HW-SW-MAPPING ClusterRef="/ECU0/SoC0/CPUCluster1">
        <Core-Runnable-Mapping CoreId="0" RunnableRef="/CPUComponent1/CPUBehavior/Runnable_HandlingHWIPsEvents"    Load="1000" Priority="1"/>
    </HW-SW-MAPPING>     

    <HW-SW-MAPPING ClusterRef="/ECU1/SoC1/CPUCluster1">
        <Core-Runnable-Mapping CoreId="0" RunnableRef="/CPUComponent2/CPUBehavior/Runnable_HandlingHWIPsEvents_2"    Load="1000" Priority="1"/>
