                        </DATA-IREF>
                    </DATA-RECEIVED-EVENT>

                    <TIMING-EVENT>
                        <SHORT-NAME name="Timing16" />
                        <START-ON-EVENT-REF DEST="/CPUComponent1/CPUBehavior/Runnable_HandlingHWIPsEvents" />
                        <CUSTOM-BEHAVIOR-REF DEST="CustomSWBeh1" />
                        <PERIOD value="100" unit="ms" />
                    </TIMING-EVENT>
                    <TIMING-EVENT>
                        <SHORT-NAME name="Timing17" />
                        <START-ON-EVENT-REF DEST="/CPUComponent1/CPUBehavior/Runnable_HandlingHWIPsEvents" />
                        <CUSTOM-BEHAVIOR-REF DEST="CustomSWBeh1" />
                        <PERIOD value="150" unit="ms" />
                    </TIMING-EVENT>
                </EVENTS>
            </SWC-INTERNAL-BEHAVIOR>
